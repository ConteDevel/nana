mod io

Option = <.Some(Any) | .None>
Real = Float
Result = <.Ok(Any) | .Err(Any)>
Date = (Int, Int, Int)
User = {
    id: Int,
    name: Str,
    email: Str,
    birthdate: Date
}

main = (argv: [Str]) -> Int {
    birthdate: Date = (01, 01, 1990)
    user: User = {
        id: 1,
        name: 'John',
        email: 'john@contedevel.com',
        birthdate: birthdate
    }
    io.println(user)
    # {id:1,name:'John',email:'john@contedevel.com',birthdate:(1,1,1990)}
    ret 0
}